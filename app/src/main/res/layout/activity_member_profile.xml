<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/dark_blue"
    tools:context=".MemberProfile">


    <ImageView
        android:id="@+id/data_image"
        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="ContentDescription" />

    <androidx.cardview.widget.CardView
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginTop="30dp"
        android:layout_marginStart="30dp"
        android:backgroundTint="#41101010"
        app:cardCornerRadius="30dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">


        <ImageView
            android:id="@+id/member_data_pro_back"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_gravity="center"
            android:src="@drawable/ic_arrow_small_left"
            tools:ignore="ContentDescription" />

    </androidx.cardview.widget.CardView>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/trainer_img"
        android:layout_marginTop="280dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:background="@drawable/login_register_bg">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="10dp"

            android:orientation="vertical"
            tools:ignore="ScrollViewSize">


            <RelativeLayout
                android:id="@+id/rl_Jennifer_James"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="35dp"
                tools:ignore="ObsoleteLayoutParam,UselessParent">


                <RelativeLayout
                    android:id="@+id/ll_name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/rl_Jennifer_James"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginHorizontal="20dp"
                    android:layout_marginTop="15dp"
                    android:orientation="horizontal"
                    tools:ignore="NotSibling">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentBottom="true"
                        android:layout_marginEnd="100dp"
                        android:orientation="vertical"
                        tools:ignore="Orientation,TextSizeCheck">


                        <TextView
                            android:id="@+id/data_name"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentEnd="true"
                            android:fontFamily="@font/opensans_extrabold"
                            android:text="Jennifer James"
                            android:textColor="@color/black"
                            android:textSize="22sp"
                            android:textStyle="bold"
                            tools:ignore="HardcodedText" />

                        <TextView
                            android:id="@+id/data_email"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/trainer_name"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentEnd="true"
                            android:fontFamily="@font/nunito_medium"
                            android:text="Functional Strength"
                            android:textColor="@color/dark_cyan"
                            android:textSize="16sp"
                            android:textStyle="bold"
                            tools:ignore="HardcodedText" />
                    </LinearLayout>

                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentEnd="true"
                        android:layout_marginTop="5dp"
                        android:layout_marginEnd="20dp"
                        android:orientation="horizontal"
                        tools:ignore="Orientation">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/ic_chat"
                            tools:ignore="ContentDescription" />
                    </RelativeLayout>

                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_about"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/ll_name"
                    android:layout_marginTop="20dp"
                    android:layout_marginHorizontal="20dp"
                    tools:ignore="UselessLeaf">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <androidx.appcompat.widget.AppCompatTextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="About"
                            android:textColor="@color/black"
                            android:textSize="20sp"
                            android:fontFamily="@font/opensans_semicondensed_semibold"
                            tools:ignore="HardcodedText" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="30dp"
                            android:layout_marginTop="20dp"
                            android:fontFamily="@font/opensans_bold"
                            android:text="Gender"
                            android:textColor="@color/text"
                            android:textSize="16sp"
                            tools:ignore="HardcodedText,SpUsage" />

                        <TextView
                            android:id="@+id/data_gender"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="20dp"
                            android:layout_marginTop="10dp"
                            android:layout_marginEnd="15dp"
                            android:fontFamily="@font/nunito_regular"
                            android:padding="10dp"
                            android:paddingStart="15dp"
                            android:text="Gender"
                            android:textColor="@color/black"
                            android:textSize="18sp"
                            tools:ignore="HardcodedText,RtlSymmetry,SpUsage,TouchTargetSizeCheck,TextSizeCheck" />

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="5dp"
                            android:src="@drawable/ic_line"
                            tools:ignore="ContentDescription" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="30dp"
                            android:layout_marginTop="20dp"
                            android:fontFamily="@font/opensans_bold"
                            android:text="Age"
                            android:textColor="@color/text"
                            android:textSize="16sp"
                            tools:ignore="HardcodedText,SpUsage" />

                        <TextView
                            android:id="@+id/data_age"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="20dp"
                            android:layout_marginTop="10dp"
                            android:layout_marginEnd="15dp"
                            android:fontFamily="@font/nunito_regular"
                            android:padding="10dp"
                            android:paddingStart="15dp"
                            android:text="Age"
                            android:textColor="@color/black"
                            android:textSize="18sp"
                            tools:ignore="HardcodedText,RtlSymmetry,SpUsage,TouchTargetSizeCheck,TextSizeCheck" />

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="5dp"
                            android:src="@drawable/ic_line"
                            tools:ignore="ContentDescription" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="30dp"
                            android:layout_marginTop="20dp"
                            android:fontFamily="@font/opensans_bold"
                            android:text="Goal"
                            android:textColor="@color/text"
                            android:textSize="16sp"
                            tools:ignore="HardcodedText,SpUsage" />

                        <TextView
                            android:id="@+id/data_goal"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="20dp"
                            android:layout_marginTop="10dp"
                            android:layout_marginEnd="15dp"
                            android:fontFamily="@font/nunito_regular"
                            android:padding="10dp"
                            android:paddingStart="15dp"
                            android:text="Goal"
                            android:textColor="@color/black"
                            android:textSize="18sp"
                            tools:ignore="HardcodedText,RtlSymmetry,SpUsage,TouchTargetSizeCheck" />

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="5dp"
                            android:src="@drawable/ic_line"
                            tools:ignore="ContentDescription" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="30dp"
                            android:layout_marginTop="20dp"
                            android:fontFamily="@font/opensans_bold"
                            android:text="Activity level"
                            android:textColor="@color/text"
                            android:textSize="16sp"
                            tools:ignore="HardcodedText,SpUsage" />

                        <TextView
                            android:id="@+id/data_activity"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="20dp"
                            android:layout_marginTop="10dp"
                            android:layout_marginEnd="15dp"
                            android:fontFamily="@font/nunito_regular"
                            android:padding="10dp"
                            android:paddingStart="15dp"
                            android:text="Activity level"
                            android:textColor="@color/black"
                            android:textSize="18sp"
                            tools:ignore="HardcodedText,RtlSymmetry,SpUsage,TouchTargetSizeCheck" />

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="5dp"
                            android:src="@drawable/ic_line"
                            tools:ignore="ContentDescription" />


                    </LinearLayout>

                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_below="@+id/rl_about"
                    android:layout_alignParentStart="true"
                    tools:ignore="ObsoleteLayoutParam">


                    <androidx.cardview.widget.CardView
                        android:id="@+id/cv_Weight"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="20dp"
                        android:layout_marginStart="30dp"
                        android:layout_marginEnd="30dp"
                        android:layout_marginTop="30dp"
                        android:layout_marginBottom="20dp"
                        app:cardCornerRadius="25dp"
                        android:backgroundTint="@color/card_view"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentStart="true"
                        app:cardElevation="10dp">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginTop="10dp"
                            android:layout_marginBottom="10dp">

                            <RelativeLayout
                                android:id="@+id/rl_all_text"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_alignParentTop="true"
                                android:layout_alignParentEnd="true"
                                android:layout_alignParentStart="true">

                                <TextView
                                    android:id="@+id/Weight"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentEnd="true"
                                    android:layout_alignParentStart="true"
                                    android:layout_alignParentTop="true"
                                    android:layout_marginStart="20dp"
                                    android:layout_marginTop="20dp"
                                    android:textColor="@color/black"
                                    android:text="@string/weight"
                                    android:fontFamily="@font/nunito_bold"
                                    android:textSize="20sp"
                                    android:textStyle="bold"
                                    tools:ignore="HardcodedText" />

                                <RelativeLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_below="@+id/Weight"
                                    android:layout_marginTop="10dp"
                                    android:orientation="horizontal"
                                    tools:ignore="UselessLeaf">

                                    <RelativeLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginEnd="200dp"
                                        android:orientation="vertical"
                                        android:layout_alignParentTop="true"
                                        android:layout_alignParentStart="true"
                                        android:layout_alignParentEnd="true"
                                        tools:ignore="UselessParent">

                                        <androidx.appcompat.widget.AppCompatTextView
                                            android:id="@+id/tvCurrent"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:text="@string/current"
                                            android:textSize="14sp"
                                            android:textColor="@color/text"
                                            android:fontFamily="@font/nunito_bold"
                                            android:layout_marginStart="20dp"
                                            android:layout_alignParentStart="true"
                                            tools:ignore="HardcodedText,TooDeepLayout" />

                                        <TextView
                                            android:id="@+id/data_cur_weight"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:text="60.6kg"
                                            android:textSize="20sp"
                                            android:textColor="@color/black"
                                            android:fontFamily="@font/nunito_bold"
                                            android:layout_marginStart="20dp"
                                            android:layout_below="@+id/tvCurrent"
                                            android:layout_alignParentStart="true"
                                            android:layout_alignParentBottom="true"
                                            tools:ignore="HardcodedText,TooDeepLayout" />
                                    </RelativeLayout>

                                    <RelativeLayout
                                        android:id="@+id/rl_hori_he"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="180dp"
                                        android:orientation="horizontal"
                                        android:layout_alignParentTop="true"
                                        android:layout_alignParentStart="true"
                                        android:layout_alignParentEnd="true"
                                        tools:ignore="UselessParent">

                                        <TextView
                                            android:id="@+id/tvHeaviest"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginEnd="80dp"
                                            android:layout_alignParentStart="true"
                                            android:layout_alignParentEnd="true"
                                            android:fontFamily="@font/nunito_bold"
                                            android:text="@string/heaviest"
                                            android:textColor="@color/text"
                                            android:textSize="14sp"
                                            tools:ignore="HardcodedText,TooDeepLayout" />

                                        <androidx.appcompat.widget.AppCompatTextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:text="100.00kg"
                                            android:fontFamily="@font/nunito_bold"
                                            android:textColor="@color/black"
                                            android:textSize="14sp"
                                            android:layout_marginEnd="20dp"
                                            android:layout_alignParentEnd="true"
                                            android:layout_alignParentBottom="true"
                                            tools:ignore="HardcodedText,TooDeepLayout" />
                                    </RelativeLayout>

                                    <RelativeLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="180dp"
                                        android:orientation="horizontal"
                                        android:layout_below="@+id/rl_hori_he"
                                        android:layout_alignParentStart="true"
                                        android:layout_alignParentEnd="true"
                                        tools:ignore="UselessParent">

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginEnd="80dp"
                                            android:layout_alignParentStart="true"
                                            android:layout_alignParentEnd="true"
                                            android:text="@string/lightest"
                                            android:textColor="@color/text"
                                            android:fontFamily="@font/nunito_bold"
                                            android:textSize="14sp"
                                            tools:ignore="HardcodedText,TooDeepLayout" />

                                        <androidx.appcompat.widget.AppCompatTextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:text="50.00kg"
                                            android:fontFamily="@font/nunito_bold"
                                            android:textColor="@color/black"
                                            android:textSize="14sp"
                                            android:layout_marginEnd="20dp"
                                            android:layout_alignParentEnd="true"
                                            android:layout_alignParentBottom="true"
                                            tools:ignore="HardcodedText,TooDeepLayout" />
                                    </RelativeLayout>

                                </RelativeLayout>

                            </RelativeLayout>


                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/rl_all_text"
                                android:layout_alignParentEnd="true"
                                android:layout_alignParentStart="true"
                                android:layout_marginBottom="10dp">

                                <com.github.mikephil.charting.charts.LineChart
                                    android:id="@+id/lineChart"
                                    android:layout_width="300dp"
                                    android:layout_height="200dp"
                                    android:layout_marginBottom="20dp"
                                    android:layout_marginHorizontal="20dp"
                                    android:layout_alignParentBottom="true"
                                    android:layout_alignParentTop="true"
                                    android:layout_alignParentEnd="true"
                                    android:layout_alignParentStart="true"
                                    android:layout_gravity="center"
                                    tools:ignore="MissingClass" />


                            </RelativeLayout>

                        </RelativeLayout>

                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="20dp"
                        android:layout_marginStart="30dp"
                        android:layout_marginEnd="30dp"
                        android:layout_marginVertical="40dp"
                        app:cardCornerRadius="25dp"
                        android:backgroundTint="@color/card_view"
                        android:layout_below="@+id/cv_Weight"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentStart="true"
                        app:cardElevation="10dp">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_alignParentTop="true"
                            android:layout_alignParentEnd="true"
                            android:layout_alignParentBottom="true"
                            android:layout_alignParentStart="true"
                            android:layout_marginBottom="20dp"
                            tools:ignore="UselessLeaf">

                            <TextView
                                android:id="@+id/totle_bmi_rep"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentEnd="true"
                                android:layout_alignParentStart="true"
                                android:layout_alignParentTop="true"
                                android:layout_marginStart="20dp"
                                android:layout_marginTop="20dp"
                                android:text="@string/bmi"
                                android:textColor="@color/black"
                                android:fontFamily="@font/nunito_bold"
                                android:textSize="20sp"
                                android:textStyle="bold"
                                tools:ignore="HardcodedText" />

                            <RelativeLayout
                                android:id="@+id/rl_BMI"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_alignParentStart="true"
                                android:layout_alignParentEnd="true"
                                android:layout_marginTop="80dp">

                                <!--                                <androidx.appcompat.widget.AppCompatTextView-->
                                <!--                                    android:layout_width="wrap_content"-->
                                <!--                                    android:layout_height="wrap_content"-->
                                <!--                                    android:text="15.7"-->
                                <!--                                    android:textColor="@color/black"-->
                                <!--                                    android:fontFamily="@font/nunito_bold"-->
                                <!--                                    android:textSize="20sp"-->
                                <!--                                    android:layout_marginStart="20dp"-->
                                <!--                                    android:textStyle="bold"-->
                                <!--                                    tools:ignore="HardcodedText" />-->

                                <RelativeLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content">


                                    <androidx.cardview.widget.CardView
                                        android:id="@+id/tv_result"
                                        android:layout_width="24dp"
                                        android:layout_height="24dp"
                                        android:layout_alignParentStart="true"
                                        android:layout_marginStart="140dp"
                                        android:elevation="8dp"
                                        app:cardCornerRadius="25dp" />
                                    <!--                                        android:backgroundTint="#E53935"-->

                                    <TextView
                                        android:id="@+id/value_text"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_alignParentEnd="true"
                                        android:layout_marginStart="10dp"
                                        android:layout_marginEnd="10dp"
                                        android:layout_toEndOf="@+id/tv_result"
                                        android:fontFamily="@font/nunito_bold"
                                        android:text="Severely underweight"
                                        android:textColor="@color/black"
                                        android:textSize="16sp"
                                        tools:ignore="HardcodedText" />

                                </RelativeLayout>

                            </RelativeLayout>

                            <RelativeLayout
                                android:id="@+id/rl_halfGauge"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="20dp"
                                android:layout_alignParentEnd="true"
                                android:layout_alignParentStart="true"
                                android:layout_below="@+id/rl_BMI">

                                <com.ekn.gruzer.gaugelibrary.HalfGauge
                                    android:id="@+id/halfGauge"
                                    android:layout_width="match_parent"
                                    android:layout_height="200dp"
                                    android:layout_alignParentTop="true"
                                    android:layout_alignParentBottom="true"
                                    android:layout_alignParentEnd="true"
                                    android:layout_alignParentStart="true"
                                    android:layout_marginStart="20dp"
                                    tools:ignore="MissingClass" />

                            </RelativeLayout>

                            <RelativeLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="20dp"
                                android:layout_marginTop="10dp"
                                android:layout_below="@+id/rl_halfGauge"
                                android:layout_alignParentBottom="true"
                                android:layout_alignParentEnd="true"
                                android:layout_alignParentStart="true"
                                tools:ignore="NotSibling">

                                <androidx.appcompat.widget.AppCompatTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="@string/height"
                                    android:textStyle="bold"
                                    android:layout_alignParentStart="true"
                                    android:layout_alignParentBottom="true"
                                    android:layout_alignParentTop="true"
                                    android:textSize="20sp"
                                    android:textColor="@color/text"
                                    android:fontFamily="@font/nunito_bold"
                                    tools:ignore="HardcodedText,RelativeOverlap"/>

                                <TextView
                                    android:id="@+id/data_cur_height"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="224cm"
                                    android:textSize="25sp"
                                    android:textColor="@color/black"
                                    android:fontFamily="@font/nunito_bold"
                                    android:layout_marginEnd="20dp"
                                    android:layout_alignParentEnd="true"
                                    android:layout_alignParentBottom="true"
                                    android:layout_alignParentTop="true"
                                    tools:ignore="HardcodedText,SpUsage" />
                            </RelativeLayout>

                        </RelativeLayout>
                    </androidx.cardview.widget.CardView>

                </RelativeLayout>


            </RelativeLayout>

        </RelativeLayout>

    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>
